#!/bin/bash

# Get the C file name from the variable
cfile="$CFILE"

# Extract the base name of the C file (without extension)
base_name=$(basename "$cfile" .c)

# Set the output file name with the .s extension
output_file="${base_name}.s"

# Generate the assembly code using GCC
gcc -S -masm=intel "$cfile" -o "$output_file"

echo "Assembly code generated for $cfile and saved in $output_file"
